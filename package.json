{
  "name": "@norefx/ngx-destroy-module",
  "version": "6.1.0",
  "description": "Guard to destroy Angular lazy-loaded route modules",
  "private": true,
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve destroy-module-demo",
    "build": "ng build destroy-module",
    "build-prod": "ng build destroy-module --prod",
    "test": "yarn run test-lib",
    "test-coverage": "yarn run test-lib-coverage",
    "test-watch": "yarn run test-lib-watch",
    "test-app": "jest --config ./libs/destroy-module-demo/jest.app.config.js --rootDir ./",
    "test-app-coverage": "jest --coverage --config ./libs/destroy-module-demo/jest.app.config.js --rootDir ./",
    "test-app-watch": "jest -o --watch --config ./libs/destroy-module-demo/jest.app.config.js --rootDir ./",
    "test-lib": "jest --config ./libs/destroy-module/jest.lib.config.js --rootDir ./",
    "test-lib-coverage": "jest --coverage --config ./libs/destroy-module/jest.lib.config.js --rootDir ./",
    "test-lib-watch": "jest -o --watch --config ./libs/destroy-module/jest.lib.config.js --rootDir ./",
    "lint": "ng lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@angular/animations": "~6.0.0",
    "@angular/common": "~6.0.0",
    "@angular/compiler": "~6.0.0",
    "@angular/core": "~6.0.0",
    "@angular/forms": "~6.0.0",
    "@angular/http": "~6.0.0",
    "@angular/platform-browser": "~6.0.0",
    "@angular/platform-browser-dynamic": "~6.0.0",
    "@angular/router": "~6.0.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-builders/jest": "~6.0.0",
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "~6.0.0",
    "@angular/language-service": "~6.0.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/jest": "~23.3.14",
    "@types/node": "~8.9.4",
    "babel-core": "~6.0.0",
    "babel-jest": "^23.0.0",
    "codelyzer": "~4.2.1",
    "editorconfig": "^0.15.3",
    "husky": "^8.0.1",
    "jest": "~23.4.2",
    "jest-preset-angular": "~6.0.0",
    "ng-packagr": "^3.0.0-rc.2",
    "prettier": "^2.7.1",
    "ts-node": "~5.0.1",
    "tsickle": "0.27.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
