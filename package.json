{
  "name": "@norefx/ngx-destroy-module",
  "version": "7.1.0",
  "description": "Guard to destroy Angular lazy-loaded route modules",
  "private": true,
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve destroy-module-demo",
    "build": "ng build destroy-module",
    "test": "yarn run test-lib",
    "test-coverage": "yarn run test-lib-coverage",
    "test-watch": "yarn run test-lib-watch",
    "test-app": "jest --config ./libs/destroy-module-demo/jest.app.config.js --rootDir ./",
    "test-app-coverage": "jest --coverage --config ./libs/destroy-module-demo/jest.app.config.js --rootDir ./",
    "test-app-watch": "jest -o --watch --config ./libs/destroy-module-demo/jest.app.config.js --rootDir ./",
    "test-lib": "jest --config ./libs/destroy-module/jest.lib.config.js --rootDir ./",
    "test-lib-coverage": "jest --coverage --config ./libs/destroy-module/jest.lib.config.js --rootDir ./",
    "test-lib-watch": "jest -o --watch --config ./libs/destroy-module/jest.lib.config.js --rootDir ./",
    "lint": "ng lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-builders/jest": "^7.0.0",
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~7.0.1",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/jest": "^24.0.0",
    "@types/node": "~8.9.4",
    "babel-core": "~6.0.0",
    "babel-jest": "~23.0.0",
    "codelyzer": "~4.5.0",
    "editorconfig": "^0.15.3",
    "husky": "^8.0.1",
    "jest": "^24.0.0",
    "jest-preset-angular": "~7.1.0",
    "ng-packagr": "~4.3.0",
    "prettier": "^2.7.1",
    "ts-node": "~7.0.0",
    "tsickle": "~0.33.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "3.1.3"
  }
}
