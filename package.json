    "test-app-watch": "jest -o --watch --config ./libs/destroy-module-demo/jest.app.config.js --rootDir ./",
    "test-lib": "jest --config ./libs/destroy-module/jest.lib.config.js --rootDir ./",
    "test-lib-coverage": "jest --coverage --config ./libs/destroy-module/jest.lib.config.js --rootDir ./",
    "test-lib-watch": "jest -o --watch --config ./libs/destroy-module/jest.lib.config.js --rootDir ./",
    "lint": "ng lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
